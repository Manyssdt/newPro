<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>ContosoRetailDW</TargetDatabaseName>
    <DeployScriptFileName>ContosoRetailDW.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=sqlplayer.database.windows.net;Persist Security Info=False;User ID=kamil;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=True;TrustServerCertificate=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <AllowIncompatiblePlatform>False</AllowIncompatiblePlatform>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="CRM">
      <Value>CRM</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="roUser">
      <Value>kamiln</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>